<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/moreIndicatorItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_more_indicator_holo_dark" />
    </item>
    <item android:id="@id/bluetoothItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_sound_bluetooth_holo_dark" />
    </item>
    <item android:id="@id/handsetItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_sound_handset_holo_dark" />
    </item>
    <item android:id="@id/speakerphoneOnItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_sound_speaker_pressed_yep" />
    </item>
    <item android:id="@id/speakerphoneOffItem">
        <bitmap android:gravity="center" android:src="@drawable/ic_sound_speaker_yep" />
    </item>
</layer-list>